{
  "name": "zeromaps-rpc",
  "version": "1.2.94",
  "description": "ZeroMaps RPC Protocol - 基于 IPv6 池和 curl-impersonate 的高性能数据获取系统",
  "type": "module",
  "scripts": {
    "proto:build": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./proto --ts_proto_opt=esModuleInterop=true ./proto/zeromaps-rpc.proto",
    "build": "tsc",
    "server": "tsx server/index.ts",
    "client": "tsx client/index.ts",
    "example": "tsx examples/basic-usage.ts"
  },
  "keywords": [
    "zeromaps",
    "rpc",
    "ipv6",
    "curl-impersonate",
    "google-earth"
  ],
  "author": "Stone",
  "license": "MIT",
  "dependencies": {
    "fastq": "^1.19.1",
    "protobufjs": "^7.4.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.13",
    "ts-proto": "^2.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}